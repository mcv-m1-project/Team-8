function [NormalFiles,LargerFiles,ImgDataset] = findMultiSignalImages(fileList,ImgDataset)

NormalFiles = [];
LargerFiles = [];

for i=1:length(fileList)
    
    SigEnd=strfind(content,sprintf('\n'));
    if length(SigEnd)>1
        LargerFiles = [LargerFiles; fileList(i)];
    else
        NormalFiles = [NormalFiles; fileList(i)];
        ImgDataset(i).PhotoRealClass = content(SigEnd(1)-1);
    end
end
        